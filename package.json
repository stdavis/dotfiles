{
  "name": "@stdavis/dotfiles",
  "description": "bootstrap and keep up-to-date my macOS machines",
  "version": "0.0.0",
  "author": "Scott Davis (stdavis@utah.gov)",
  "bin": {
    "dotfiles": "./bin/run"
  },
  "bugs": "https://github.com/stdavis/dotfiles/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/stdavis/dotfiles",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "dotfiles",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "stdavis/dotfiles",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2"
  }
}
