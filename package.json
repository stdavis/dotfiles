{
  "name": "@stdavis/dotfiles",
  "description": "bootstrap and keep up-to-date my macOS machines",
  "version": "0.1.2",
  "author": "Scott Davis (stdavis@utah.gov)",
  "bin": {
    "dotfiles": "bin/run"
  },
  "bugs": "https://github.com/stdavis/dotfiles/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^11.0.3",
    "prettier": "^2.3.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "eslintConfig": {
    "extends": "prettier",
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    }
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/stdavis/dotfiles",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "dotfiles",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "repository": "stdavis/dotfiles",
  "scripts": {
    "format": "prettier --write src",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
